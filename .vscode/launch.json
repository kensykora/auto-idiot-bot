{
    "version": "0.2.0",
    "configurations": [{
        "name": "Launch via NPM",
        "type": "node",
        "request": "launch",
        "cwd": "${workspaceRoot}",
        "runtimeExecutable": "npm",
        "runtimeArgs": [
            "run", "serve-debug"
        ],
        "sourceMaps": true,
        "outFiles": [ "${workspaceFolder}/dist/**/*.js" ],
        "port": 5858,
        "envFile": "${workspaceFolder}/.env"
    }]
}